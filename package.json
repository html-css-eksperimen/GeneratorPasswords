{
  "name": "password-generator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port=4200",
    "build": "vue-cli-service build",
    "buildmodern": "vue-cli-service build --modern",
    "buildmoderncss": "vue-cli-service build --modern && npm run purgecss",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "lintfix": "vue-cli-service lint --fix",
    "postbuildcss": "npm run purgecss",
    "purgecss": "node_modules/purgecss/bin/purgecss --config ./purgecss.config.js --out ./dist/css",
    "static-serve": "cd dist && live-server --port=4200 --host=localhost --entry-file=/index.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "core-js": "^3.5.0",
    "register-service-worker": "^1.6.2",
    "sweetalert2": "^9.5.3",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.1",
    "@vue/cli-plugin-e2e-cypress": "^4.1.1",
    "@vue/cli-plugin-eslint": "^4.1.1",
    "@vue/cli-plugin-pwa": "^4.1.1",
    "@vue/cli-plugin-router": "^4.1.1",
    "@vue/cli-plugin-unit-mocha": "^4.1.1",
    "@vue/cli-plugin-vuex": "^4.1.1",
    "@vue/cli-service": "^4.1.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "chai": "^4.1.2",
    "compression-webpack-plugin": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "live-server": "^1.2.1",
    "purgecss": "^1.4.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "vue-template-compiler": "^2.6.10"
  }
}
